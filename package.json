{
    "name": "ev-assets-manager",
    "version": "1.0.0",
    "description": "EV Assets Manager - Backend APIs for managing charging assets",
    "main": "node ev-charging-api/server.js",
    "scripts": {
        "lint": "eslint .",
        "start": "node ev-charging-api/server.js",
        "test": "mocha --exit ev-charging-api/test/location.test.js &&  mocha --exit ev-charging-api/test/chargeStation.test.js && mocha --exit ev-charging-api/test/chargePoint.test.js && mocha --exit ev-charging-api/test/connector.test.js",
        "coverage": "nyc --reporter=lcov --reporter=text npm run test",
        "jscpd": "jscpd --threshold 2 --ignore 'node_modules/**'"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mongoose": "^7.0.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.20.0",
        "chai": "^4.3.6",
        "chai-http": "^5.1.1",
        "eslint": "^8.57.1",
        "globals": "^15.15.0",
        "jscpd": "^3.3.26",
        "mocha": "^11.1.0",
        "nyc": "^17.1.0",
        "sinon": "^19.0.2",
        "supertest": "^7.0.0",
        "typescript-eslint": "^8.24.1"
    },
    "nyc": {
        "reporter": [
            "json-summary",
            "text",
            "lcov"
        ],
        "all": true,
        "include": [
            "ev-charging-api/**/*.js"
        ]
    },
    "engines": {
        "node": ">=18"
    },
    "license": "MIT"
}
